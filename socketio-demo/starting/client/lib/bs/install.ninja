rescript = 1
rule cp
  command = cp $i $out
rule touch
 command = touch $out
o index.cmi : cp ../bs/src/index.cmi
o index.cmj : cp ../bs/src/index.cmj
o index.cmt : cp ../bs/src/index.cmt
o index.res : cp ../../src/index.res
o deleteme.cmi : cp ../bs/src/deleteme.cmi
o deleteme.cmj : cp ../bs/src/deleteme.cmj
o deleteme.cmt : cp ../bs/src/deleteme.cmt
